<wpfLikeAvaloniaNavigation:Page xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:wpfLikeAvaloniaNavigation="clr-namespace:WpfLikeAvaloniaNavigation;assembly=WpfLikeAvaloniaNavigation"
             mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="300" Height="300" Width="500"
             x:Class="PracticalWork4_Gusenkov_Permyakova.Pages.NavigationPage" Background="#222831" Foreground="#EEEEEE">
    <Grid RowDefinitions="Auto, Auto, *" >
        <TextBlock Text="Страница навигации" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,20,0,0"/>
        <Border Grid.Row="1" HorizontalAlignment="Center" Margin="0,20,0,0" BorderThickness="4" BorderBrush="#393e46" CornerRadius="6">
            <StackPanel Spacing="25" Margin="10,15,10,10">
                <Button Content="Вычисление первой функции" Width="300" Foreground="#EEEEEE"  Background="#00adb5" x:Name="FuncOne" Click="FuncOne_OnClick" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                <Button Content="Вычисление второй функции" Width="300" Foreground="#EEEEEE"  Background="#00adb5" x:Name="FuncTwo" Click="FuncTwo_OnClick" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                <Button Content="Вычисление третьей функции" Width="300" Foreground="#EEEEEE"  Background="#00adb5" x:Name="FuncThree" Click="FuncThree_OnClick" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
            </StackPanel>
        </Border>
    </Grid>
</wpfLikeAvaloniaNavigation:Page>
